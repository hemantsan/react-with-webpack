{
    "name": "react-with-webpack",
    "version": "1.0.0",
    "description": "React with webpack setup",
    "main": "index.js",
    "scripts": {
        "start": "npm run build",
        "build": "webpack -d && webpack-dev-server --content-base src/ --inline --hot",
        "build:prod": "webpack -p && copy src/index.html dist/index.html"
    },
    "keywords": [
        "Reactjs",
        "Webpack"
    ],
    "author": "Hemant Sankhla",
    "license": "MIT",
    "dependencies": {
        "react": "^16.2.0",
        "react-dom": "^16.2.0"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.9.7"
    }
}